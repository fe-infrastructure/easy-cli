{
  "name": "@eye-socket/easy-cli",
  "type": "module",
  "version": "0.0.1",
  "description": "A simple scaffolding for generating front-end development templates",
  "author": "yanquanfahei <1449826851@qq.com>",
  "license": "MIT",
  "keywords": [
    "scaffold",
    "front-end",
    "templates",
    "cli"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "@eye-socket/easy-cli": "index.js"
  },
  "files": [
    "index.js",
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "prompts": "^2.4.2",
    "read-pkg": "^7.1.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@eye-socket/eslint-config-ts": "^0.0.2",
    "@types/node": "^18.13.0",
    "@types/prompts": "^2.4.2",
    "@types/validate-npm-package-name": "^4.0.0",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1"
  }
}
